{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI: Debug",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",        // Replace 'main' with your filename (without .py)
        "--host", "127.0.0.1",
        "--port", "8000",
        "--reload"
      ],
      "jinja": true,
      "env": {
        "ENV": "development"
      },
      "console": "integratedTerminal"
    }
  ]
}
